<!-- %SIGNATURE% -->
<appSettings>
  <app name="rabbit.host" value="%RABBIT_HOST%" />
  <app name="service.endpoint" value="%SERVICE_URL%" />
</appSettings>

__CONFIG__
---
env: ['dev', 'qa', 'prod'] 
vars:
  RABBIT_HOST:
    dev:  rabbitmq.local
    qa:   rabbitmq.qa.local
    prod: rabbitmq.prod.com
  SERVICE_URL:
    dev:  http://host.dev
    qa:   http://host.qa
    prod: http://host.prod
  SIGNATURE:
   default: updated by <%= ENV['USER'] %> on <%= Time.now %>
